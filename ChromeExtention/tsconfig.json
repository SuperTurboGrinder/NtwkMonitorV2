{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "strictNullChecks": false,
    "module": "es2015",
    "target": "es5",
    "rootDir": ".",
    "outDir": "app",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  },
  "include": [
    "src/*.ts",
    "src/**/*.ts",
    "test/*.ts",
    "test/**/*.ts"
  ]
}
